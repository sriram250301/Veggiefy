    <fragment
        android:id="@+id/nav_helpandsupport"
        android:name="in.veggiefy.androidapp.ui.maindashboard.helpandsupport.HelpAndSupportFragment"
        android:label="fragment_help_and_support"
        tools:layout="@layout/fragment_help_and_support" />
    <activity
        android:id="@+id/nav_logout"
        android:name="in.veggiefy.androidapp.ui.logout.Logout"
        android:label="activity_logout"
